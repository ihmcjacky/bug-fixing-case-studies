{
  "name": "client",
  "version": "2.0.6",
  "nodejs": "true",
  "web_accessible_resources": [
    "*.html",
    "*.js",
    "*.css",
    "*.svg",
    "*.png",
    "*.jpg",
    "*.jpeg"
  ],
  "jest": {
    "transform": {
      "\\.[jt]s?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/src/test/__mocks__/styleMock.js",
      "^material-ui-nested-menu-item": "<rootDir>/node_modules/material-ui-nested-menu-item/dist-web/index.js.test"
    }
  },
  "output": "./dist/prod",
  "homepage": "/",
  "private": true,
  "node-remote": "<all_urls>",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/react-hooks": "^5.1.1",
    "@testing-library/user-event": "^7.2.1",
    "@welldone-software/why-did-you-render": "^4.2.7",
    "bezier-js": "^6.1.4",
    "classnames": "^2.2.6",
    "customize-cra": "^1.0.0",
    "d3": "^5.16.0",
    "file-saver": "^2.0.2",
    "fontsource-roboto": "^2.1.4",
    "html-react-parser": "^0.13.0",
    "html2canvas": "^1.0.0-rc.7",
    "i18next": "^19.6.2",
    "i18next-xhr-backend": "^3.2.2",
    "immer": "^8.0.1",
    "jest-css-modules-transform": "^4.3.0",
    "jquery": "^3.5.1",
    "js-cookie": "^2.2.1",
    "jszip": "^3.5.0",
    "material-design-icons": "^3.0.1",
    "material-ui-nested-menu-item": "^1.0.2",
    "moment": "^2.27.0",
    "pixi.js": "github:Kethen/pixijs#v8_webpack4",
    "rainbowvis.js": "^1.0.1",
    "react": "^16.13.1",
    "react-app-rewired": "^2.2.1",
    "react-beautiful-dnd": "^11.0.5",
    "react-circular-progressbar": "^2.0.3",
    "react-data-export": "^0.6.0",
    "react-devtools": "^4.24.7",
    "react-dom": "^16.13.1",
    "react-draggable": "^4.4.3",
    "react-dropzone": "^11.0.3",
    "react-flip-move": "^3.0.4",
    "react-i18next": "^11.7.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-select": "^3.1.0",
    "react-simpletabs": "git+https://github.com/YOUR_ORGANIZATION/react-simpletabs.git",
    "react-swipeable-views": "^0.13.9",
    "recharts": "^1.8.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.1.1",
    "underscore": "^1.10.2",
    "use-immer": "^0.4.2",
    "xlsx": "^0.16.7"
  },
  "scripts": {
    "start": "NODE_OPTIONS=--openssl-legacy-provider react-app-rewired start",
    "start-win": "set NODE_OPTIONS=--openssl-legacy-provider && react-app-rewired start",
    "build": "NODE_OPTIONS=--openssl-legacy-provider GENERATE_SOURCEMAP=false react-app-rewired build",
    "test": "NODE_OPTIONS=--openssl-legacy-provider node ./dev/nw-test && react-app-rewired test",
    "test-win": "set NODE_OPTIONS=--openssl-legacy-provider && react-app-rewired test",
    "eject": "NODE_OPTIONS=--openssl-legacy-provider react-scripts eject",
    "nw-dev": "node ./dev/nw-dev",
    "nw-dev-win": "node ./dev/nw-dev-win",
    "nw-build": "node ./dev/nw-build",
    "nw-build-win": "node ./dev/nw-build-win",
    "testCI": "CI=true npm test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "cheerio": "1.0.0-rc.12",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "mocha": "^8.1.3",
    "nwjs-builder-phoenix": "^1.15.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "sinon": "^9.0.3"
  }
}
